new;
cls;

//Draw scatterplot of random multivariate data
//seed random generator with current time (hundredths of a second)
t = time;
rndseed t[4];

//covariance matrix
cov =  { 1 0.9, 0.9 1 };
//vector of means
mu = { 2, 2 };
pts = rndMVn( 300, mu, cov);
x = pts[.,1];
y = pts[.,2];

plotScatter( x, y );
